
public class Ficha {
	private int F_cem;
	private int F_cinquenta;
	private int F_vinte;
	private int F_dez;
	private int F_cinco;
	private int F_um;
	
	
	public Ficha(){ 
	  F_cem =2;
	  F_cinquenta=2;
	  F_vinte=5;
	  F_dez=5;
	  F_cinco=8;
	  F_um=10;
	}

	public void Ganha_Ficha(Ficha aposta) {
		this.F_cem+=aposta.F_cem;
		this.F_cinquenta+=aposta.F_cinquenta;
		this.F_vinte+=aposta.F_vinte;
		this.F_dez+=aposta.F_dez;
		this.F_cinco+=aposta. F_cinco;
		this.F_um+=aposta.F_um;
	}

	public void Perde_Ficha(Ficha aposta) {
		this.F_cem-=aposta.F_cem;
		this.F_cinquenta-=aposta.F_cinquenta;
		this.F_vinte-=aposta.F_vinte;
		this.F_dez-=aposta.F_dez;
		this.F_cinco-=aposta. F_cinco;
		this.F_um-=aposta.F_um;
		
	}		
}
